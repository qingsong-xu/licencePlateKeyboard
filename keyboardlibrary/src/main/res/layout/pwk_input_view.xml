<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvEnergy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="新能源"
        android:textColor="@color/color_energy"
        android:textSize="9sp"
        app:layout_constraintLeft_toLeftOf="@id/et_car_license_inputbox8"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/et_car_license_inputbox1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="省"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/et_car_license_inputbox2"
        app:layout_constraintTop_toBottomOf="@+id/tvEnergy"
        app:layout_constraintVertical_weight="1" />

    <TextView
        android:id="@+id/et_car_license_inputbox2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="A"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/et_car_license_inputbox1"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@id/et_car_license_inputbox1"
        app:layout_constraintRight_toLeftOf="@id/llDot"
        app:layout_constraintTop_toTopOf="@id/et_car_license_inputbox1"
        app:layout_constraintVertical_weight="1" />

    <LinearLayout
        android:id="@+id/llDot"
        android:layout_width="20dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@id/et_car_license_inputbox1"
        app:layout_constraintLeft_toRightOf="@id/et_car_license_inputbox2"
        app:layout_constraintRight_toLeftOf="@id/et_car_license_inputbox3"
        app:layout_constraintTop_toTopOf="@id/et_car_license_inputbox1"
        app:layout_constraintVertical_weight="1">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dot_black" />
    </LinearLayout>

    <TextView
        android:id="@+id/et_car_license_inputbox3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="1"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/et_car_license_inputbox1"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@id/llDot"
        app:layout_constraintRight_toLeftOf="@id/et_car_license_inputbox4"
        app:layout_constraintTop_toTopOf="@id/et_car_license_inputbox1"
        app:layout_constraintVertical_weight="1" />

    <TextView
        android:id="@+id/et_car_license_inputbox4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/et_car_license_inputbox1"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@id/et_car_license_inputbox3"
        app:layout_constraintRight_toLeftOf="@id/et_car_license_inputbox5"
        app:layout_constraintTop_toTopOf="@id/et_car_license_inputbox1"
        app:layout_constraintVertical_weight="1" />

    <TextView
        android:id="@+id/et_car_license_inputbox5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="3"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/et_car_license_inputbox1"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@id/et_car_license_inputbox4"
        app:layout_constraintRight_toLeftOf="@id/et_car_license_inputbox6"
        app:layout_constraintTop_toTopOf="@id/et_car_license_inputbox1"
        app:layout_constraintVertical_weight="1" />

    <TextView
        android:id="@+id/et_car_license_inputbox6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="4"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/et_car_license_inputbox1"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@id/et_car_license_inputbox5"
        app:layout_constraintRight_toLeftOf="@id/et_car_license_inputbox7"
        app:layout_constraintTop_toTopOf="@id/et_car_license_inputbox1"
        app:layout_constraintVertical_weight="1" />

    <TextView
        android:id="@+id/et_car_license_inputbox7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="5"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/et_car_license_inputbox1"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@id/et_car_license_inputbox6"
        app:layout_constraintRight_toLeftOf="@id/et_car_license_inputbox8"
        app:layout_constraintTop_toTopOf="@id/et_car_license_inputbox1"
        app:layout_constraintVertical_weight="1" />

    <TextView
        android:id="@+id/et_car_license_inputbox8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="6"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/et_car_license_inputbox1"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@id/et_car_license_inputbox7"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/et_car_license_inputbox1"
        app:layout_constraintVertical_weight="1" />
</androidx.constraintlayout.widget.ConstraintLayout>